{
  "$schema": "https://developer.microsoft.com/json-schemas/spfx-build/config.2.0.schema.json",
  "version": "2.0",
  "bundles": {
    "webshoptwo-web-part": {
      "components": [
        {
          "entrypoint": "./lib/webparts/webshoptwo/WebshoptwoWebPart.js",
          "manifest": "./src/webparts/webshoptwo/WebshoptwoWebPart.manifest.json"
        }
      ]
    }
  },
  "externals": {
    "jquery": {
      "path": "https://ajax.googleapis.com/ajax/libs/jquery/3.1.1/jquery.min.js",
      "globalName": "jQuery"
    }
  },
  "localizedResources": {
    "WebshoptwoWebPartStrings": "lib/webparts/webshoptwo/loc/{locale}.js",
    "ControlStrings": "node_modules/@pnp/spfx-controls-react/lib/loc/{locale}.js"
  }
}